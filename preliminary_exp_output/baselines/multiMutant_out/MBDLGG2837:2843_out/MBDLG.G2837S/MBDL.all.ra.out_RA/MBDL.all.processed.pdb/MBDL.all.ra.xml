<kinariMol-config>
  <input>
    <pdb path="MBDL.all.cur.out/MBDL.all.processed.pdb.knr" />
    <bondfiles singleCovBonds="MBDL.all.cur.out/MBDL.all.SingleCovBonds.Pruned.bnd.knr" 
	       doubleCovBonds="MBDL.all.cur.out/MBDL.all.DoubleCovBonds.Pruned.bnd.knr" 
	       hBonds="MBDL.all.cur.out/MBDL.all.HBonds.Pruned.bnd.knr" 
	       hPhobes="MBDL.all.cur.out/MBDL.all.HPhobes.Pruned.bnd.knr" 
	       rBonds="MBDL.all.cur.out/MBDL.all.RBonds.Pruned.bnd.knr"/>
  </input>
  <modeling>
    <modeler type="BARHEURISTIC" />
  </modeling>
  <analysis>
    <dilutionAnalysis run="false" />
    <generateMetricsBBH run="true" />
    <generateMetricsMolFW run="true" />
    <generateMetricsBBHPDB run="true" />
  </analysis>
  <output>
    <outputDir path="MBDL.all.ra.out/" />
    <mol-fr-xml output="true" /> 
    <mol-fr-pdb output="true" /> 
    <result-pdb output="true" /> 
    <pre-peb-game-bbh output="true" /> 
    <post-peb-game-bbh output="true" /> 
    <pre-peb-game-assoc-graph output="true" /> 
    <post-peb-game-assoc-graph output="true" /> 
    <rigid-cluster-decomposition output="true" /> 
  </output>
</kinariMol-config>
