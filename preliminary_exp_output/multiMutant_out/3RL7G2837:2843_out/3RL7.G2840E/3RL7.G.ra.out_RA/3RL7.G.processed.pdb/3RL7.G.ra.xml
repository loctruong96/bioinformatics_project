<kinariMol-config>
  <input>
    <pdb path="3RL7.G.cur.out/3RL7.G.processed.pdb.knr" />
    <bondfiles singleCovBonds="3RL7.G.cur.out/3RL7.G.SingleCovBonds.Pruned.bnd.knr" 
	       doubleCovBonds="3RL7.G.cur.out/3RL7.G.DoubleCovBonds.Pruned.bnd.knr" 
	       hBonds="3RL7.G.cur.out/3RL7.G.HBonds.Pruned.bnd.knr" 
	       hPhobes="3RL7.G.cur.out/3RL7.G.HPhobes.Pruned.bnd.knr" 
	       rBonds="3RL7.G.cur.out/3RL7.G.RBonds.Pruned.bnd.knr"/>
  </input>
  <modeling>
    <modeler type="BARHEURISTIC" />
  </modeling>
  <analysis>
    <dilutionAnalysis run="false" />
    <generateMetricsBBH run="true" />
    <generateMetricsMolFW run="true" />
    <generateMetricsBBHPDB run="true" />
  </analysis>
  <output>
    <outputDir path="3RL7.G.ra.out/" />
    <mol-fr-xml output="true" /> 
    <mol-fr-pdb output="true" /> 
    <result-pdb output="true" /> 
    <pre-peb-game-bbh output="true" /> 
    <post-peb-game-bbh output="true" /> 
    <pre-peb-game-assoc-graph output="true" /> 
    <post-peb-game-assoc-graph output="true" /> 
    <rigid-cluster-decomposition output="true" /> 
  </output>
</kinariMol-config>
