<kinariMol-config>
  <input>
    <pdb path="MBDL.G.cur.out/MBDL.G.processed.pdb.knr" />
    <bondfiles singleCovBonds="MBDL.G.cur.out/MBDL.G.SingleCovBonds.Pruned.bnd.knr" 
	       doubleCovBonds="MBDL.G.cur.out/MBDL.G.DoubleCovBonds.Pruned.bnd.knr" 
	       hBonds="MBDL.G.cur.out/MBDL.G.HBonds.Pruned.bnd.knr" 
	       hPhobes="MBDL.G.cur.out/MBDL.G.HPhobes.Pruned.bnd.knr" 
	       rBonds="MBDL.G.cur.out/MBDL.G.RBonds.Pruned.bnd.knr"/>
  </input>
  <modeling>
    <modeler type="BARHEURISTIC" />
  </modeling>
  <analysis>
    <dilutionAnalysis run="false" />
    <generateMetricsBBH run="true" />
    <generateMetricsMolFW run="true" />
    <generateMetricsBBHPDB run="true" />
  </analysis>
  <output>
    <outputDir path="MBDL.G.ra.out/" />
    <mol-fr-xml output="true" /> 
    <mol-fr-pdb output="true" /> 
    <result-pdb output="true" /> 
    <pre-peb-game-bbh output="true" /> 
    <post-peb-game-bbh output="true" /> 
    <pre-peb-game-assoc-graph output="true" /> 
    <post-peb-game-assoc-graph output="true" /> 
    <rigid-cluster-decomposition output="true" /> 
  </output>
</kinariMol-config>
